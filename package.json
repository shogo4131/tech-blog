{
  "name": "tech-blog",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "dev": "run-p -cl dev:*",
    "dev:next": "next dev && node createData.js",
    "build": "pathpida && next build && node createData.js && next export && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint --cache",
    "lint:fix": "next lint --fix",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "stylelint --fix src/**/*.css",
    "prettier": "prettier --check .  --cache",
    "prettier:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "generate:path": "pathpida --watch --output src/lib",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "regression": "reg-suit run",
    "screenshot": "storycap --serverCmd \"npx http-server storybook-static -a localhost -p 6006\" http://localhost:6006",
    "prepare": "husky install",
    "createData": "node createData.js"
  },
  "dependencies": {
    "axios": "^1.2.5",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.2.1",
    "destyle.css": "^4.0.0",
    "highlight.js": "^11.7.0",
    "microcms-js-sdk": "^2.5.0",
    "next": "^13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gtm-module": "^2.0.11",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@netlify/plugin-nextjs": "^4.38.1",
    "@storybook/addon-actions": "^7.0.23",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/nextjs": "^7.0.25",
    "@storybook/react": "^7.0.23",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@types/cheerio": "^0.22.31",
    "@types/highlight.js": "^10.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^9.1.2",
    "eslint": "8.33.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "netlify-cli": "^14.0.0",
    "next-sitemap": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.4",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.12.2",
    "reg-publish-s3-plugin": "^0.12.1",
    "reg-suit": "^0.12.1",
    "scaffdog": "^2.5.1",
    "storybook": "^7.0.22",
    "storybook-css-modules-preset": "^1.1.1",
    "storycap": "^4.1.1",
    "stylelint": "^14.16.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "5.1.6"
  }
